<% reserve.each do |r| %>
	<div class="row table">
		<div class="col-lg-2"><%= link_to r.reservable.name, r.reservable %></div>
  	<div class="col-lg-2"><%= render partial: "shared/show_vm_state", locals: {vm: r.reservable} %></div>
  	<div class="col-lg-3"><%=r.reservable.vmresourcepool.name%></div>
  	<div class="col-lg-3"><%=r.reservable.vmresourcepool.vmhost.name%></div>
  	
  	<% if show_user %>
  		<div class="col-lg-2">
  			<%=r.event.user.lastname%> <%=r.event.user.firstname%>
  		</div>
  	<%end%>
  	
  
  	
	</div>
<% end %>